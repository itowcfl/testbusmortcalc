<aura:component implements="lightning:availableForFlowScreens" access="global">
    <aura:attribute name="businessLoanRate" type="Decimal" access="global"/>
    <aura:attribute name="businessLoanYears" type="Integer" access="global"/>
    <aura:attribute name="businessLoanPrincipal" type="Decimal" access="global"/>

    <c:businessLoanAmortizationSchedule principal="{!v.businessLoanPrincipal}" rate="{!v.businessLoanRate}" years="{!v.businessLoanYears}"/>
</aura:component>	
